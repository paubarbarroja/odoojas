<odoo>
    <data>

       <template id="berp_20191103_1522_website" name="form_add_socio">
            <t t-call="website.layout">
                <div id="wrap">
                    <div class="oe_structure">
                        <section class="container readable o_website_form_builder pt32 pb32">
                            <div class="container">
                                <div class="row">
                                    <div class="pt0 col-lg-12 pb0">
                                        <h1 class="s_title_default" style="text-align: center; color: rgb(33, 37, 41);">FES-TE SOCI !</h1>
                                        <h2 class="s_title_default" style="text-align: center; color: rgb(33, 37, 41);">FES ATLETISME AMB NOSALTRES<hr/></h2>
                                        <br/><br/>
                                        <form accept-charset="UTF-8" action="/website_form/" class="s_website_form container-fluid mt32" data-model_name="crm.lead" data-force_action="" data-success_page="/" enctype="multipart/form-data" method="post">
                                            <div class="o_website_form_fields">
                                                <div class="o_not_editable d-none o_default_snippet_text">
                                                    This placeholder prevents its parent from
                                                    disappearing when emptied
                                                </div>
                                                <h4 class="s_title_default" style="text-align: left; color: rgb(33, 37, 41);">Dades Generals<hr/></h4>
                                                <div class="form-field form-field-char form-group o_required" data-model-field="true" data-optional="false">
                                                    <label class="control-label" for="name"><span>Nom Complet</span></label>
                                                    <input name="name" required="required" class="form-control o_website_form_input" type="text"/>
                                                </div>
                                                <div class="form-group form-field  form-field-char" data-model-field="true" data-optional="true">
                                                    <label class="control-label" for="phone">
                                                        <span>Nº Telèfon/Mòvil</span>
                                                    </label>
                                                    <input name="phone" class="form-control o_website_form_input" type="text"/>
                                                </div>
                                                <div class="form-group form-field  form-field-char" data-model-field="true" data-optional="true">
                                                    <label class="control-label" for="email_from">
                                                        <span>Correu Electronic</span>
                                                    </label>
                                                    <input name="email_from" class="form-control o_website_form_input" type="text"/>
                                                    <p class="help-block">
                                                        <br/>
                                                    </p>
                                                </div>
                                                <div class="form-group form-field  form-field-selection-radio" data-model-field="false" data-optional="true">
                                                    <label class="control-label" for="Campo Selección única personalizado">
                                                        <span>Genere</span>
                                                    </label>
                                                    <div class="radio">
                                                        <label class="control-label">
                                                        <input name="Genere" type="radio" class="o_website_form_input" value="Masculí"/>
                                                        <span>Masculí</span>
                                                        </label>
                                                    </div>
                                                    <div class="radio">
                                                        <label class="control-label">
                                                        <input name="Genere" type="radio" class="o_website_form_input" value="Femeni"/>
                                                        <span>Femeni</span>
                                                        </label>
                                                    </div> 
                                                </div>
                                                <div class="form-group form-field  form-field-char" data-model-field="true" data-optional="true">
                                                    <label class="control-label" for="contact_name">
                                                        <span>DNI</span>
                                                    </label>
                                                    <input name="contact_name" class="form-control o_website_form_input" type="text"/>
                                                </div>
                                                <div class="form-group form-field  form-field-date" data-model-field="true" data-optional="true">
                                                    <label class="control-label" for="date_deadline">
                                                        <span>Data Naixement</span>
                                                    </label>
                                                    <input name="date_deadline" class="form-control o_website_form_input o_website_form_date" type="text"/>
                                                    <p class="help-block">
                                                        <br/>
                                                    </p>
                                                </div>
                                                <br/>
                                                <h4 class="s_title_default" style="text-align: left; color: rgb(33, 37, 41);">Direcció Postal<hr/></h4>
                                                <div class="form-group form-field  form-field-char" data-model-field="true" data-optional="true">
                                                    <label class="control-label" for="street">
                                                        <span>Adreça</span>
                                                    </label>
                                                    <input name="street" class="form-control o_website_form_input" type="text"/>
                                                </div>
                                                <div class="form-group form-field  form-field-char" data-model-field="true" data-optional="true">
                                                    <label class="control-label" for="zip">
                                                        <span>Codi postal</span>
                                                    </label>
                                                    <input name="zip" class="form-control o_website_form_input" type="text"/>
                                                </div>
                                                <div class="form-group form-field  form-field-char" data-model-field="true" data-optional="true">
                                                    <label class="control-label" for="city">
                                                        <span>Població</span>
                                                    </label>
                                                    <input name="city" class="form-control o_website_form_input" type="text"/>
                                                </div>
                                                <br/>
                                                <h4 class="s_title_default" style="text-align: left; color: rgb(33, 37, 41);">Dades Bancaries<hr/></h4>
                                                <div class="form-group form-field  form-field-char" data-model-field="true" data-optional="true">
                                                    <label class="control-label" for="function">
                                                        <span>Compte Bancari</span>
                                                    </label>
                                                    <input name="function" class="form-control o_website_form_input" type="text"/>
                                                </div>
                                                <div class="form-group form-field  form-field-char" data-model-field="true" data-optional="true">
                                                    <label class="control-label" for="referred">
                                                        <span>Banc</span>
                                                    </label>
                                                    <input name="referred" class="form-control o_website_form_input" type="text"/>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <button type="button" class="btn btn-primary btn-lg o_website_form_send o_default_snippet_text">
                                                    Enviar
                                                </button>
                                                <span id="o_website_form_result"/>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </div>
            </t>
        </template>


    </data>
</odoo>