<odoo>
    <data>

        <template id="berp_20201121_2107_website" name="Formulari Renovació Fitxa">
            <t t-call="website.layout">
                <div id="wrap" class="oe_structure oe_empty">
                    <section class="container readable o_website_form_builder">
                        <br/>
                        <br/>
                        <b>
                            <h1 style="text-align:center">FORMULARI RENOVACIÓ FITXA</h1>
                        </b>
                        <br/>
                        <form accept-charset="UTF-8" action="/website_form/" class="s_website_form col-md-12 mt32" data-model_name="berp.solicitud_renovacion_ficha" data-force_action="" data-success_page="https://www.jasabadell.com/shop/product/renovacio-fitxa-72?category=22" enctype="multipart/form-data" method="post">
                            <div class="o_website_form_fields">
                                <div class="form-group form-field  form-field-char o_required" data-model-field="true" data-optional="true">
                                    <label class="control-label" for="nombre">
                                        <span> Nom </span>
                                    </label>
                                    <input name="nombre" class="form-control o_website_form_input" type="text"/>
                                </div>
                                <div class="form-group form-field  form-field-char o_required" data-model-field="true" data-optional="true">
                                    <label class="control-label" for="apellido1">
                                        <span> Primer Cognom </span>
                                    </label>
                                    <input name="apellido1" class="form-control o_website_form_input" type="text"/>
                                </div>
                                <div class="form-group form-field  form-field-char o_required" data-model-field="true" data-optional="true">
                                    <label class="control-label" for="apellido2">
                                        <span> Segon Cognom </span>
                                    </label>
                                    <input name="apellido2" class="form-control o_website_form_input" type="text"/>
                                </div>
                                <div class="form-group form-field  form-field-char o_required" data-model-field="true" data-optional="true">
                                    <label class="control-label" for="email">
                                        <span> Correu electrònic </span>
                                    </label>
                                    <input name="email" class="form-control o_website_form_input" type="text"/>
                                </div>
                                <div class="form-group form-field  form-field-char o_required" data-model-field="true" data-optional="true">
                                    <label class="control-label" for="phone">
                                        <lt-highlighter style="display: none;">
                                            <lt-div spellcheck="false" class="lt-highlighter__wrapper" style="width: 0px !important; height: 0px !important; transform: none !important; transform-origin: 0px 0px !important; zoom: 1 !important; margin-top: 2px !important;">
                                                <lt-div class="lt-highlighter__scrollElement" style="top: 0px !important; left: 0px !important;"/>
                                            </lt-div>
                                        </lt-highlighter>
                                        <span spellcheck="false" data-gramm="false"> Nº telèfon/mòbil </span>
                                    </label>
                                    <input name="phone" class="form-control o_website_form_input" type="text"/>
                                </div>
                                <div class="form-group form-field  form-field-selection o_required" data-model-field="true" data-optional="true">
                                    <label class="control-label" for="ficha">
                                        <lt-highlighter style="display: none;">
                                            <lt-div spellcheck="false" class="lt-highlighter__wrapper" style="width: 0px !important; height: 0px !important; transform: none !important; transform-origin: 0px 0px !important; zoom: 1 !important; margin-top: 2px !important;">
                                                <lt-div class="lt-highlighter__scrollElement" style="top: 0px !important; left: 0px !important;"/>
                                            </lt-div>
                                        </lt-highlighter>
                                        <span spellcheck="false" data-gramm="false">
                                            Tipus de fitxa
                                        </span>
                                    </label>
                                    <select name="ficha" class="form-control o_website_form_input">
                                        <option value="territorial">
                                                Territorial
                                        </option>
                                        <option value="nacional" selected="selected">
                                                Nacional
                                        </option>
                                    </select>
                                </div>
                                <div class="row form-group form-field o_website_form_custom o_fake_not_editable o_website_form_required_custom">
